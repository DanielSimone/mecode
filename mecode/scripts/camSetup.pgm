'DVAR $StartXPosition
'DVAR $MasterStartPosition
'DVAR $MasterMotionRange
'DVAR $Slave1StartPosition
'DVAR $Slave2StartPosition
'DVAR $Slave3StartPosition
'DVAR $Slave4StartPosition
'DVAR $Slave5StartPosition
